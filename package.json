{
  "name": "app",
  "version": "0.1.0",
  "description": "Using Angular 2 with Electron",
  "main": "index.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"electron .\"",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build": "webpack --config config/webpack.dev.js --watch",
    "build:once": "webpack --config config/webpack.dev.js",
    "package": "electron-builder .",
    "build:prod": "webpack --config config/webpack.prod.js",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenisVuyka/ng2-electron.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "Angular"
  ],
  "author": "Denys Vuika <denys.vuika@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DenisVuyka/ng2-electron/issues"
  },
  "homepage": "https://github.com/DenisVuyka/ng2-electron/#readme",
  "dependencies": {
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@angular/router": "4.4.4",
    "@angular/upgrade": "4.4.4",
    "@types/html2canvas": "0.0.35",
    "bootstrap": "^3.3.7",
    "core-js": "2.5.1",
    "css-loader": "^2.1.0",
    "jquery": "^3.3.1",
    "jspm-nodelibs-fs": "^0.2.1",
    "ngx-bootstrap-modal": "^2.0.1",
    "node-gyp": "^3.8.0",
    "node-pre-gyp": "^0.12.0",
    "path": "^0.12.7",
    "popper.js": "^1.14.7",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.3",
    "sqlite3": "^4.0.6",
    "style-loader": "^0.23.1",
    "to-string-loader": "^1.1.5",
    "zone.js": "0.8.26"
  },
